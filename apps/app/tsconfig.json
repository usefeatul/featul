{
  "extends": "@featul/tsconfig/nextjs.json",
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.d.ts",
    ".next/dev/types/**/*.ts",
    "next.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}